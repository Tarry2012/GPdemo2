<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="dao.InterestDAO">

    <resultMap id="UserInterestMap" type="domain.UserInterestDO">
        <result column="id" property="Id"/>
        <result column="user_id" property="userId"/>
        <result column="interest_id" property="interestId"/>
    </resultMap>


    <insert id="add" parameterType="domain.InterestDO">
        <selectKey resultType="int" keyProperty="Id">
            SELECT LAST_INSERT_ID()
        </selectKey>
        insert into wd_user_interest(user_id,interest_id) values(#{userId},#{interestId})
    </insert>

</mapper>