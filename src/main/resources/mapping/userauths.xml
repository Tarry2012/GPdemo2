<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dao.UserAuthsDAO">
    <resultMap id="UserAuthsMap" type="domain.UserAuthsDO">
        <result column="id" property="id"/>
        <result column="user_id" property="userId"/>
        <result column="login_type" property="loginType"/>
        <result column="login_name" property="loginName"/>
        <result column="login_password" property="loginPassword"/>

    </resultMap>

    <select id="getById" parameterType="Map" resultMap="UserAuthsMap">
        select * from wd_user_auths where user_id=#{userId} and login_type=#{loginType}
    </select>

    <select id="getAllMail" resultMap="List">
        
    </select>

</mapper>